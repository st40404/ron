
#####   install hp driver    #####

1.sudo apt install libcanberra-gtk-module libcanberra-gtk3-module

2.wget https://download.sourceforge.net/project/hplip/hplip/3.21.2/hplip-3.21.2.run

3.sh hplip-3.21.2.run

4.hp-setup  (ip:163.13.133.211)


#####   unstall hplip    #####

1.sudo apt-get purge hplip hplip-data hplip-doc hplip-gui hpijs-ppds \
libsane-hpaio printer-driver-hpcups printer-driver-hpijs

2.sudo rm -rf /usr/share/hplip/

